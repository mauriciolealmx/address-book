{
  "name": "address-book",
  "version": "0.1.0",
  "description": "An address book using Node and Express",
  "engines": {
    "node": "5.9.1"
  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src -d dist ",
    "prestart": "npm run build",
    "start:live": "nodemon ./src/index.js --exec babel-node -- --presets=es2015",
    "start": "node ./dist/index",
    "test": "TESTING=true mocha ./test/top.js --compilers js:babel-register --timeout 15000",
    "test:live": "TESTING=true mocha ./test/top.js --compilers js:babel-register --watch --timeout 15000"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "node",
    "express"
  ],
  "dependencies": {
    "bluebird": "^3.3.5",
    "body-parser": "1.7.0",
    "chai": "^3.5.0",
    "cookie-parser": "1.3.2",
    "ejs": "2.4.1",
    "express": "4.13.3",
    "express-session": "1.7.6",
    "firebase-admin": "^4.2.1",
    "jsonwebtoken": "^7.3.0",
    "nock": "^9.0.13",
    "pg": "^6.1.2",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.23.1",
    "babel-eslint": "4.1.8",
    "babel-istanbul": "^0.8.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.24.1",
    "babel-watch": "^2.0.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.18.7",
    "rimraf": "^2.5.4"
  },
  "license": "MIT"
}
